{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"strict": true,
		"jsx": "preserve",
		"importHelpers": true,
		"skipDefaultLibCheck": true,
		"esModuleInterop": true,
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"sourceMap": true,
		"baseUrl": ".",
		"types": ["react"],
		"lib":["ESNext","DOM","DOM.Iterable","ScriptHost"],
		"composite": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
	},
	"include": ["vite.config.ts"]
  }
